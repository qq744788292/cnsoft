        </if>
        <if test=' jobUseTime != null and jobUseTime !="" '><!-- 运行时间（秒） -->
            ,job_use_time = #{jobUseTime}
        </if>
        <if test=' jobRunStatus != null and jobRunStatus !="" '><!-- 运行状态 -->
            ,job_run_status = #{jobRunStatus}
