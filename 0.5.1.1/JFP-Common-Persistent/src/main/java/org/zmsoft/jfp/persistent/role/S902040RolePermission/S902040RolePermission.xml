            <if test=' userId != null and userId !="" '><!-- 用户ID -->
                AND user_id = #{userId}
            </if>
            <if test=' roleName != null and roleName !="" '><!-- 角色名称 -->
                AND INSTR(role_name , #{roleName} ) != 0
            </if>
            <if test=' userName != null and userName !="" '><!-- 用户名称 -->
                AND INSTR(user_name , #{userName} ) != 0
