    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='S902040RolePermissionDBO' resultType='S902040RolePermissionDBO'>
        SELECT <include refid='tableColumns'/> FROM s902040_role_permission
        WHERE 1 = 1 
            <if test=' roleId != null and roleId !="" '><!-- 角色ID -->
                AND role_id = #{roleId}
